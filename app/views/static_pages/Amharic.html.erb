<style type="text/css">
.btn_lang {
  background-color: #028fcc;
  border: none;
  color: white;
  text-align: center;
  font-size: 16px;
  padding: 20px;
  width: 120px;
  transition: all 0.5s;
  cursor: pointer;
  margin: 5px;
   float: right;
  position: fixed;
  bottom: 20px;
  right: 30px;
  z-index: 99;
    border-radius: 10px;
}

.btn_lang span {
  cursor: pointer;
  display: inline-block;
  position: relative;
  transition: 0.5s;
}

.btn_lang span:after {
  content: '»';
  position: absolute;
  opacity: 0;
  top: 0;
  right: -20px;
  transition: 0.5s;
}

.btn_lang:hover span {
  padding-right: 25px;
}

.btn_lang:hover span:after {
  opacity: 1;
  right: 0;
}
</style>

